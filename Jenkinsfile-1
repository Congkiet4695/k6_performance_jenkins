pipeline {
    agent any
    stages {
        stage('Performance Testing') {
            steps {
                echo 'Run jmeter'
                sh 'cd ../../../home/cong-kiet/jmenter/apache-jmeter-5.5/bin'
                sh 'ls -a'
                sh './jmeter -jjmeter.save.saveservice.output_format=xml -n -t ../../test/create_gr.jmx -l ../../test/create_gr.jtl'
            }
        }
    }
}